{
  "name": "glucose-monitor",
  "version": "1.0.0",
  "description": "Tracking for Calories/A1C/Glucose",
  "author": "Ryan Conrad <camalot@gmail.com>",
  "license": "MIT",
  "main": "./app/www.js",
  "scripts": {
    "sync": "node package-sync.js",
    "build": "npm run sync && npm install --prefix ./app && tsc && copyfiles -u 2 ./src/assets/**/* ./app/assets/ && copyfiles -u 2 ./src/views/**/* ./app/views/ && copyfiles -u 0 .env ./app/",
    "start": "npm run build && node ./app/www.js",
    "exec": "npm run build && npm update && nodemon ./app/www.js",
    "test": "mocha"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "body-parser": "^1.20.2",
    "cli-color": "^2.0.4",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-handlebars": "^8.0.2",
    "mathjs": "^14.4.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.48",
    "mongodb": "^6.8.0",
    "nodemon": "^3.1.4"
  },
  "devDependencies": {
    "@types/axios": "^0.14.4",
    "@types/cli-color": "^2.0.6",
    "@types/express": "^5.0.1",
    "@types/node": "^22.14.1",
    "chai": "^4.2.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "mocha": "^10.1.0",
    "supertest": "^3.4.2",
    "supertest-as-promised": "^4.0.2"
  }
}
